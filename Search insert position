class Solution {
    public int searchInsert(int[] nums, int t) {
        int temp=0;
        for(int i=0;i<nums.length;i++)
        {
            temp=i;
            if(nums[i]==t)
            {
                return i;
            }
            else if(nums[i]>t)
            {
                return i;
            }
        }
        return temp+1;
    }
}
